{
  "name": "controle-financeiro-backend",
  "version": "1.0.0",
  "description": "API Backend para Sistema de Controle Financeiro Universal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "check": "node scripts/check-setup.js",
    "setup": "node scripts/setup-db.js",
    "setup:force": "node scripts/setup-db.js --force",
    "setup:seed": "node scripts/setup-db.js --seed",
    "migration:run": "sequelize-cli db:migrate",
    "migration:undo": "sequelize-cli db:migrate:undo",
    "seed:run": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "finance",
    "control",
    "api",
    "nodejs",
    "express"
  ],
  "author": "Seu Nome",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "compression": "^1.7.4",
    "express-async-errors": "^3.1.1",
    "pdfkit": "^0.14.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "sequelize-cli": "^6.6.2",
    "@types/jest": "^29.5.11",
    "axios": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}